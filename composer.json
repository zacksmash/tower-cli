{
    "name": "zacksmash/tower-cli",
    "description": "Tower CLI",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/zacksmash",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "knplabs/github-api": "^2.13",
        "laravel-zero/framework": "^8.0",
        "mockery/mockery": "^1.3.1",
        "php-http/guzzle6-adapter": "^1.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/tower"]
}
